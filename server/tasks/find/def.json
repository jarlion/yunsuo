{
  "code": "find",
  "name": "查找",
  "desc": "查找文件或目录",
  "params": [
    {
      "name": "patterns",
      "type": "string[]",
      "required": true,
      "desc": "文件或目录路径匹配正则表达式数组",
      "default": [],
      "placeholder": "输入正则表达式数组"
    },
    {
      "name": "types",
      "type": "string[]",
